<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <Copyright>(c) 2019, Pawel Gerr. All rights reserved.</Copyright>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Authors>Pawel Gerr</Authors>    
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageIconUrl>http://thinktecture.com/images/Logo_T_Nontransparent.png</PackageIconUrl>
    <PackageProjectUrl>https://dev.azure.com/pawelgerr/Thinktecture.EntityFrameworkCore</PackageProjectUrl>
    <PackageLicenseUrl>https://dev.azure.com/pawelgerr/_git/Thinktecture.EntityFrameworkCore?path=%2FLICENCE.md</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git@ssh.dev.azure.com:v3/pawelgerr/Thinktecture.EntityFrameworkCore/Thinktecture.EntityFrameworkCore</RepositoryUrl>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <RootNamespace>Thinktecture</RootNamespace>
    <LangVersion>7.3</LangVersion>
    <NoWarn>$(NoWarn);CA1303;CA1062;CA1707;CA1801</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2019.1.3">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.4">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
